    <!-- Data Table area Start-->
    <div class="data-table-area">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="data-table-list">
                        <div class="basic-tb-hd">
                            <h2>List Alumni Data</h2>
                            <p>Find your Alumni Data.</p>
                        </div>
                        <div class="table-responsive">
                            <table id="data-table-basic" class="table table-striped">
                                <thead>
                                    <tr>
                                        <th>Nama Lengkap</th>
                                        <th>Email</th>
                                        <th>Daerah Asal</th>
                                        <th>Alamat</th>
                                        <th>Nomor Telepon</th>
                                        <th>Jenis Kelamin</th>
                                        <th>Tanggal Lahir</th>
                                        <th>Status</th>
                                        <th>Jurusan</th>
                                        <th>Detail Jurusan</th>
                                        <th>Tahun Kelulusan</th>
                                        <th>Job Areas</th>
                                        <th>Job Details</th>
                                        <th>Edit Data</th>
                                        <th>Remove Data</th>
                                    </tr>
                                </thead>
                                <tbody>
                                <!-- Looping Alumni List -->
                                <% if (data) { %>
                                <% data.forEach(function(alumni){ %>
                                    <tr>
                                        <td><%= alumni.namaLengkap %></td>
                                        <td><%= alumni.email %></td>
                                        <td><%= alumni.daerahAsal %></td>
                                        <td><%= alumni.alamat %></td>
                                        <td><%= alumni.nomorTelepon %></td>
                                        <td><%= alumni.jenisKelamin %></td>
                                        <td><%= alumni.tanggalLahir %></td>
                                        <td><%= alumni.status %></td>
                                        <td><%= alumni.jurusan %></td>
                                        <td><%= alumni.detailJurusan %></td>
                                        <td><%= alumni.tahunKelulusan %></td>
                                        <td><%- alumni.pekerjaan %></td>
                                        <td><%- alumni.pekerjaanDetails %></td>
                                        <td>
                                            <a href="/alumni/edit/<%= alumni.id %>"><button class="btn btn-primary notika-btn-primary">Edit Data</button></a>
                                        </td>
                                        <td>
                                            <form action="/alumni/delete/<%= alumni.id %>" method="POST">
                                                <!-- <button type="submit" name="_method" class="btn btn-primary notika-btn-primary" style="background-color:#d82436;">Remove Data</button> -->
                                                <form method="post" action="/alumni/delete/<%= alumni.id %>" style="float:right">
                                                    <button type="submit" name="delete" value='Delete' onClick="return confirm('Are you sure you want to delete this alumni data?')" class="btn btn-primary notika-btn-primary" style="background-color:#d82436;">Remove Data</button>                                                    
                                                    <input type="hidden" name="_method" value="DELETE" />
                                                </form>
                                            </form>
                                        </td>
                                    </tr>
                                <% }) %>
                                <% } %>
                                </tbody>
                                <tfoot>
                                    <tr>
                                    <!--Empty Data-->
                                    </tr>
                                </tfoot>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
