<!-- Include Header -->
<%- include public-layout/header.ejs %>

<div class="container-contact100">
    <div class="wrap-contact100">
        <form class="contact100-form validate-form">
            <span class="contact100-form-title">
                <font face="Acme">AUISS Database Form</font>
            </span>

            <div style="display: block; margin-right: auto; margin-left: auto;">
                <img src="/login-assets/images/auiss-icon.png" style="height: 135px; margin-bottom: 30px" />
            </div>

            <div class="wrap-input100 validate-input bg1" data-validate="Masukkan Job Details Anda!" style="margin-bottom:45px">
                <center>
                    <span class="label-input100">Selamat Datang di AUISS Database. Silahkan untuk memilih, berdasarkan
                        status anda. #SalamKeluargaAUISS *</span>
                </center>
            </div>

            <!-- Alumni -->
            <div class="rs1-wrap-input100">
                <a href="/form/alumni">
                    <button class="contact100-form-btn" type="button">
                        <span>
                            Alumni
                            <i class="fa fa-long-arrow-right m-l-7" aria-hidden="true"></i>
                        </span>
                    </button>
                </a>
            </div>

            <!-- Student -->
            <div class="rs1-wrap-input100">
                <a href="/form/student">
                    <button class="contact100-form-btn" type="button">
                        <span>
                            Student
                            <i class="fa fa-long-arrow-right m-l-7" aria-hidden="true"></i>
                        </span>
                    </button>
                </a>
            </div>

        </form>
    </div>
</div>

<!-- Include Footer -->
<%- include public-layout/footer.ejs %>